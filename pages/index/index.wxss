/**index.wxss**/
page{
  width: 100%;
  height: 100;
}
.modle{
  width: 100%;
  padding: 5px;
}

/* 类名设置 */
.appClassTitle{
  /* background:rgba(224, 243, 52, 0.741); */
  padding: 2px;
  text-align: center;
  border-radius: 5px;
  border-bottom: rgb(173, 87, 235) double 3px;
  font-family: 'Segoe UI', Tahoma, Geneva, Verdana, sans-serif;
  font-style: normal;
  font-size: large;
}

.appClassBody{
  display: flex;
  width: 100%;
  flex-direction: row;
  flex-wrap:wrap ;
  justify-content:space-between;
}

.itemStyle{
  display: flex;
  flex-direction: column;
  align-items: center;
  text-align: center;
  width: 25%;
  margin-bottom: 3px;
}

.icon-img{
  width: 60px;
  height: 60px;
}

.icon-name{
  width: 100%;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: small;
  /* word-wrap: break-word; */
}

